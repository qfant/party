<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CirCleRefreshLayout">
        <attr name="AniBackColor" format="color" />
        <attr name="AniForeColor" format="color" />
        <attr name="CircleSmaller" format="integer"/>
    </declare-styleable>
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="TitleIndicator">
        <attr name="titlePadding" format="dimension" />
        <!-- Left/right padding of not active view titles. -->
        <attr name="clipPadding" format="dimension" />
        <attr name="textColor" format="color|reference" />
        <attr name="textSizeNormal" format="dimension" />
        <attr name="textSizeSelected" format="dimension" />
        <attr name="footerLineHeight" format="dimension" />
        <attr name="footerColor" format="color" />
        <attr name="footerTriangleHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="MEditText">
        <attr name="textHinit" format="reference" />
        <attr name="heightEqual" format="reference" />
        <attr name="widthEqual" format="reference" />
        <attr name="textMaxLength" format="integer" />
        <attr name="android:digits" />
        <attr name="android:inputType" />
        <attr name="rightSpaceNum" format="integer" />
        <attr name="inputTextSize" format="dimension" />
        <attr name="inputGravity">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
            <flag name="center_vertical" value="0x10" />
            <flag name="fill_vertical" value="0x70" />
            <flag name="center_horizontal" value="0x01" />
            <flag name="fill_horizontal" value="0x07" />
            <flag name="center" value="0x11" />
            <flag name="fill" value="0x77" />
            <flag name="clip_vertical" value="0x80" />
            <flag name="clip_horizontal" value="0x08" />
            <flag name="start" value="0x00800003" />
            <flag name="end" value="0x00800005" />
        </attr>
        <attr name="isPwd" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ClearableEditText">
        <attr name="deleteEnabled" format="boolean" />
        <attr name="textSizeHint" format="dimension" />
        <attr name="textStyleHint">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="android:textSize" />
        <attr name="android:textStyle" />
        <attr name="clearableIcon" format="integer" />
    </declare-styleable>


    <declare-styleable name="SivinBanner">
        <attr name="banner_pointContainerBackground" format="reference|color" />
        <attr name="banner_pointDrawable" format="reference" />
        <attr name="banner_pointContainerLeftRightPadding" format="dimension" />
        <attr name="banner_pointTopBottomMargin" format="dimension" />
        <attr name="banner_pointLeftRightMargin" format="dimension" />
        <attr name="banner_pointGravity">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="left" value="0x03" />
            <flag name="right" value="0x05" />
            <flag name="center_horizontal" value="0x01" />
        </attr>
        <attr name="banner_pointAutoPlayInterval" format="integer" />
        <attr name="banner_pageChangeDuration" format="integer" />
        <attr name="banner_tipTextColor" format="reference|color" />
        <attr name="banner_tipTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SwipRefreshLayout">
        <attr name="direction">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="both" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_text" format="string" />
        <attr name="civ_text_color" format="color" />
        <attr name="civ_text_size" format="float" />
    </declare-styleable>
</resources>
